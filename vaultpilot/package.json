{
  "name": "vaultpilot",
  "version": "1.0.0",
  "description": "VaultPilot - Comprehensive EvoAgentX integration for Obsidian",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "lint": "tsc --noEmit",
    "dev:watch": "concurrently \"npm run dev\" \"npm run copy:watch\"",
    "copy:watch": "nodemon --watch dist --ext js,json --exec \"npm run copy:demo\"",
    "copy:demo": "mkdir -p demo-vault/.obsidian/plugins/vaultpilot && cp dist/main.js manifest.json demo-vault/.obsidian/plugins/vaultpilot/"
  },
  "keywords": [
    "obsidian",
    "ai",
    "agent",
    "automation",
    "productivity"
  ],
  "author": "VaultPilot Team",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.5",
    "concurrently": "^8.2.1",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "obsidian": "^1.8.7",
    "rollup": "^3.26.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.5.3",
    "typescript": "^5.2.2"
  }
}
